package day0110;

public class ArraysChange_03 {

	public static void main(String[] args) {
		//순서바꾸기 연습
		
		int a=10,b=20;
		System.out.println("a="+a+", b="+b);
		int temp=a;
		a=b;
		b=temp;
		System.out.println("a="+a+", b="+b);
		
		int[] n= {1,2,3};
		
		for(int s:n)
			System.out.println();
	}
	
}

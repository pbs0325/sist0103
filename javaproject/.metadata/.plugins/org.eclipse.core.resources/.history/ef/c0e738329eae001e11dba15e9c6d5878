package day0109;

public class StringEx_07 {

	public static void main(String[] args) {
		
		System.out.println("String 객체에 대한 멤버 메서드 익혀보기");
		
		String str1="apple";
		String str2="Apple";
		String str3="Have a Nice Day";
		String str4="	Today now	";
		
		System.out.println("str3이 길이:"+str3.length());
	}
	
}

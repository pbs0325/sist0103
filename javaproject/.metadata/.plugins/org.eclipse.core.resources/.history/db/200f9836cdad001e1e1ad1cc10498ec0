package day0106;

public class For_07 {

	public static void main(String[] args) {
		//while문과 for문 비교
		
		int i=10;
		
		System.out.println("**while문으로 10~0출력**");
		while(i>=0)
		{
			System.out.println(i--+"  ");
		}
		
		System.out.println("**for문으로 10~0출력**");
		for(int a=10;a>=0;a--)
		{
			System.out.println(a+"  ");
		}
	}
	
}

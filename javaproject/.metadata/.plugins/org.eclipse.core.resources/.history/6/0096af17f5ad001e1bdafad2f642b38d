package day0108;

public class QuizForWhile_15 {

	public static void main(String[] args) {
		
		test1();
		
	}
	
	public static void test1() {
	//문제: 1~100까지의 3의 배수 갯수 구하기
		int n=0;
		int cnt=0;
		
		while(true) {
			n++;
			
			if(n%3==0)
				cnt++;
			if(n==100)
				break;
		}
		 
		
	}
	
	public static void test2() {
		
	 
		
	}	
	
	public static void test3() {
		
	 
		
	}
}

package day0108;

public class QuizFor_12 {

	public static void main(String[] args) {
		
		System.out.println("1. 1 2 4 5 7 8 10 (continue활용할 것!!)");
		
		
		for(int n=1;n<=10;n++)
		{
			if(n%3==0)
				continue;
			System.out.print(n+"  ");
		
		}
			
		for(int n=1;n<=10;n++)
		{
			if(n%2==0)
				continue;
			System.out.println("홀수값:"+n);
		}
		
	}
	
}

package day0105;

public class IfTest_09 {
	
	public static void main(String[] args) {
		
		int a=10;
		
		if(a>5) {
			System.out.println(a+"는 5보다 크다");
		}
		
		
		int b=9;
		
		if(b%2==0)
			System.out.println(b+"는 짝수입니다.");
		else
			System.out.println(b+"는 홀수입니다.");
		
			
		System.out.println("if문 종료");
		
		
	}

}

package day0104;

import java.util.Scanner;

public class KeyBoardIn_14 {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		
		String name;
		int age;
		
		System.out.print("이름을 입력해 보세요==>");
		name=sc.nextLine();
		
		System.out.print("나이를 입력하세요==>");
		age=sc.nextInt();
		
	}
	
}
